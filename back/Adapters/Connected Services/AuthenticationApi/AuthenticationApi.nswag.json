  "components": {
    "schemas": {
      "Unauthorized": {
        "type": "object",
        "required": [
          "name",
          "message",
          "status"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "The error name",
            "default": "UNAUTHORIZED",
            "minLength": 1,
            "example": "UNAUTHORIZED"
          },
          "message": {
            "type": "string",
            "description": "An error message",
            "minLength": 1
          },
          "status": {
            "type": "number",
            "description": "The status code of the exception",
            "default": 401,
            "example": 401
          },
          "errors": {
            "type": "array",
            "description": "A list of related errors",
            "items": {
              "$ref": "#/components/schemas/GenericError"
            }
          },
          "stack": {
            "type": "string",
            "description": "The stack trace (only in development mode)"
          }
        }
      },
      "GenericError": {
        "type": "object",
        "required": [
          "name",
          "message"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "The error name",
            "minLength": 1
          },
          "message": {
            "type": "string",
            "description": "An error message",
            "minLength": 1
          }
        }
      },
      "PostLoginModelWithSalt": {
        "type": "object",
        "required": [
          "salt"
        ],
        "properties": {
          "salt": {
            "type": "string",
            "minLength": 1
          }
        }
      },
      "PostLoginInitRequest": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "hash": {
            "type": "string"
          },
          "name": {
            "type": "string",
            "minLength": 1
          }
        }
